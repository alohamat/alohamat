#include <iostream>
#include <vector>
#include <numeric>

#define all(x) (x).begin(), (x).end()
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n = 3, valor = 1, soma;
    vector<vector<int>> matriz(n, vector<int>(n));
    vector<int> linha;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> matriz[i][j];
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            linha.push_back(matriz[i][j]);
        }
        soma = accumulate(all(linha), 0);
        linha.clear();
        cout << soma << "\n";
    }
    

    return 0;
}

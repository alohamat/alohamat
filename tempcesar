<html>
    <script>
        function um_a_dez() {
            for (let i = 1; i < 11; i++) {
                alert(i);
            }
        }
        function tabuada() {
            let x = Number(prompt("Digite um número para saber a tabuada:"));
            for (let i = 1; i < 11; i++) {
                alert(x + " * " + i + " = " + x * i);
            }
        }
        function pares() {
            let x = Number(prompt("Digite o número para saber pares:"));
            for (let i = 1; i < x; i++) {
                if (i % 2 == 0) alert(i + " é par!");
            }
        }
        function primo() {
            let x = Number(prompt("Digita numero"));
            for (let i = 2; i <= x/2; i++) {
                if (x % i == 0) {
                    alert("Não é primo");
                    break;
                } else alert("é primo");
            }
        }
        function media() {
            let n = Number(prompt("Digita a quantidade de números:"));
            numeros = [];
            for (let i = 1; i <= n; i++) {
                let x = prompt(i + "° número:");
                numeros.push(x);
            }
        }
    </script>
</html>
